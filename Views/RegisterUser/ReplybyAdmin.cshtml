@model IEnumerable<IceCreamProject.Models.tbl_feedback>

@{
    ViewBag.Title = "ReplybyAdmin";
    Layout = "~/Views/Shared/partial_layout.cshtml";
}
<center>
    <h2 style="color:lightpink">ReplybyAdmin</h2>
</center>

<table class="table" >
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.f_text)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.f_email)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.f_name)
        </th>
        
        <th>
            @Html.DisplayNameFor(model => model.Admin_Reply)
        </th>
                <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.f_text)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.f_email)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.f_name)
        </td>
        
        <td>
            @Html.DisplayFor(modelItem => item.Admin_Reply)
        </td>
        
    </tr>
}

</table>
